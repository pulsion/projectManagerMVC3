@model projectManagerMVC3.Models.t_staff_holiday

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

<fieldset>
    <legend>Staff Holidays</legend>

    <div class="display-label">User Id</div>
    <div class="display-field">@Model.userid</div>

    <div class="display-label">Holiday Start Date</div>
    <div class="display-field">@String.Format("{0:g}", Model.startdate)</div>

    <div class="display-label">Holiday End Date</div>
    <div class="display-field">@String.Format("{0:g}", Model.enddate)</div>
</fieldset>
<p>
    @Html.ActionLink("Edit", "Edit", new { id=Model.id }) |
    @Html.ActionLink("Back to List", "Index")
</p>


